  <DataSets>
    <DataSet Name="CustomerSales">
      <Query>
        <DataSourceName>OnlineMusicStore</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Genres">
            <Value>=Parameters!Genres.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT C.CustomerID,C.FirstName+' '+C.LastName AS Name,ISNULL(C.Gender,'N') AS Gender,YEAR(SO.OrderDate) AS Year, G.GenreName, SOD.SongPrice
FROM Customers AS C JOIN SalesOrder AS SO
	ON C.CustomerID=SO.CustomerID JOIN SalesOrderDetail AS SOD 
	ON SO.SalesOrderID=SOD.SalesOrderID JOIN Songs AS S
	ON SOD.SongID=S.SongID JOIN Genres AS G
	ON S.GenreID=G.GenreID
WHERE ( YEAR(SO.OrderDate)=@Year OR @Year IS NULL) AND 
	(G.GenreName=@Genres OR @Genres IS NULL)</CommandText>
      </Query>
      <Fields>
        <Field Name="CustomerID">
          <DataField>CustomerID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
